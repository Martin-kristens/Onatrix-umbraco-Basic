@using Umbraco.Cms.Web.Common.PublishedModels;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Cms.Core.Models.Blocks

@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage;

@{
    // hämtar blocklist från Home
    var slides = Model.Value<IEnumerable<Umbraco.Cms.Core.Models.Blocks.BlockListItem>>("slides") ?? null;
}


<div class="our-projects-partners">
    @foreach (var partner in slides)
    {
        //hämtar bild från Image slider item
        var image = partner.Content.Value<IPublishedContent>("selectedImage")?.Url();

        <img src="@image" alt="Partner Image" />
    }
</div>
